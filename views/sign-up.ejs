<main class="contenedor contenedor-formularios">
    <h1><%= pageName %></h1>
    <form action="/signUp" method="post" class="default-form">
        <div class="campo">
            <label>Email</label>
            <input type="email" name="email" placeholder="Your Email">
        </div>

        <div class="campo">
            <label>Name</label>
            <input type="text" name="name" placeholder="Your Name">
        </div>

        <div class="campo">
            <label>Password</label>
            <input type="password" name="password" placeholder="Your Password">
        </div>

        <div class="campo">
            <label>Confirm Password</label>
            <input type="password" name="passwordConfirm" placeholder="Your Password Confirmation">
        </div>

        <div class="campo enviar">
            <input type="submit" class="btn btn-rosa" value="Create Account">
        </div>
    </form>
</main>